    <body>
      <entry xml:id="abeilen.s.3v" sortKey="abeilen.abeilen.s.3v">
        <form type="lemma">
          <orth>abeilen,</orth>
        </form>
        
        <dictScrap rend="artkopf" xml:space="preserve"> <gram xml:space="preserve" type="wortart">V.</gram></dictScrap>
        
        <!-- Bedeutung  -->
        <sense xml:id="abeilen.s.3v_s0" level="1">
          <def xml:space="preserve"><dictScrap rend="def">›jm. etw. (z. B. Beute, Land) oder jn. abjagen, entreißen‹</dictScrap>; vgl. <ref type="mat" target="eilen.s.3v">eilen</ref> <ref type="mat" target="eilen.s.3v#eilen.s.3v_s4">4</ref>.</def>
          <entry rend="synt">
            <dictScrap rend="synt">mit Dat. d. P. und Akk. d. S./P.: <hi rendition="it">jm. den raub, das tor / vieh / mos / volk a.</hi></dictScrap>
          </entry>
          <!-- Belegblock -->
          <entry rend="blb" corresp="abeilen.s.3v_s0">
            <!--  Quellen  -->
            <dictScrap rend="cit">
              <cit>
                <bibl xml:space="preserve"><name n="149" cert="low">Chron. Augsb.</name> <citedRange n="149" facs="4,426,21" rend="export" target="https://archive.org/details/DieChronikenDerSchwaebischenStaedteAugsburg/page/n488">4, 426, 21</citedRange> (<region key="schwäb.">schwäb.</region><add>, zu </add><date from="1499" to="1499" confidence="1">1499</date>)</bibl>
                <quote xml:space="preserve" source="Objektsprache">da kam die welsch kard [...], di <oRef norm="abeilen" type="oVar.lem" rend="automatic" n="86">abeilten</oRef> den aidgenossen den raub. </quote>
              </cit>
              <cit>
                <bibl xml:space="preserve"><name n="1524" cert="high">Turmair</name> <citedRange n="1524" facs="4,1174,4" rend="export" target="http://daten.digitale-sammlungen.de/~db/0001/bsb00016723/images/index.html?id=00016723&amp;nativeno=1174">4, 1174, 4</citedRange> (<region key="moobd.">moobd.</region>, <date from="1522" to="1522" confidence="1">1522</date>/<date from="1533" to="1533" confidence="1">33</date>)</bibl>
                <quote xml:space="preserve" source="Objektsprache">der brach unversehen auf, übereilt die wachter, eilt in die tör ab. </quote>
              </cit>
              <cit>
                <bibl xml:space="preserve"><name n="78" cert="high">Grossmann</name>, Unrest. Öst. Chron. <citedRange n="78" facs="150,21" rend="export" target="http://daten.digitale-sammlungen.de/bsb00000691/images/index.html?id=00000691&amp;groesser=&amp;fip=eayaewqsdasxsfsdreayaqrsxdsydeayaewqsdas&amp;no=&amp;seite=200">150, 21</citedRange> (<region key="oobd.">oobd.</region>, <date from="1466" to="1499" confidence="0.0294">3. Dr. 15. Jh.</date>)</bibl>
                <quote xml:space="preserve" source="Objektsprache">Etlich [...] warn nicht bey dem <oRef norm="abeilen" type="oVar.lem" rend="automatic" n="91">abeylen</oRef> des pergs gewessen.</quote>
              </cit>
            </dictScrap>
            <!--  Belegstellen  -->
            <dictScrap rend="bls">
              <cit>
                <bibl xml:space="preserve"><name n="3704" cert="high">Chron. Nürnb.</name> <citedRange n="3704" facs="4,207,5" rend="export" target="https://books.google.de/books?id=v4XTAAAAMAAJ&amp;pg=PA207&amp;dq&amp;f=false#v=onepage&amp;q&amp;f=false">4, 207, 5</citedRange><region key="nobd."/><date from="1400" to="1499" confidence="0.01"/>; </bibl>
              </cit>
              <cit>
                <bibl xml:space="preserve"><name n="221" cert="low">Müller</name>, Grafsch. Hohenb. <citedRange n="221" facs="146,39">146, 39</citedRange><region key="schwäb."/><date from="1381" to="1454" confidence="0.0135"/>; </bibl>
              </cit>
              <cit>
                <bibl xml:space="preserve"><name n="259" cert="medium">Baumann</name>, Bauernkr. Oberschwaben <citedRange n="259" facs="92,5" rend="export" target="https://archive.org/details/quellenzurgesch00baumgoog/page/n103">92, 5</citedRange><region key="schwäb."/><date from="1532" to="1541" confidence="0.1"/>; </bibl>
              </cit>
              <cit>
                <bibl xml:space="preserve"><name n="788" cert="high">Luginbühl</name>, Brennwalds Schweizer Chron. <citedRange n="788" facs="1,288,5">1, 288, 5</citedRange><region key="halem."/><date from="1508" to="1516" confidence="0.1111"/>; <citedRange n="788" facs="2,135,16">2, 135, 16</citedRange><region key="halem."/><date from="1508" to="1516" confidence="0.1111"/>.</bibl>
              </cit>
            </dictScrap>
          </entry>
        </sense>
      </entry>
